pipeline {
	agent any
	stages {
		stage('Init') {
			steps {
				echo 'Hi, this is David'
				echo 'Let us start the test'
			}
		}
		stage('Build'){
			steps{
				echo 'We are building the project'
			}
		}
		stage('Deploy'){
			steps{
				echo 'We are deploying the project'
			}
		}
		stage('Deploy Production'){
			steps{
				echo 'Now we are deploying to the production'
			}
		}
	}
}
